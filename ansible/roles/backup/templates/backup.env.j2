BACKUP_DIR={{ backup_dir }}
BACKUP_BUCKET={{ backup_bucket }}
RETENTION_DAYS={{ backup_retention_days }}
DB_NAME={{ backup_db_name }}
DB_USER={{ backup_db_user }}
PGPASSWORD={{ backup_db_password }}
{% if backup_aws_access_key %}
AWS_ACCESS_KEY_ID={{ backup_aws_access_key }}
AWS_SECRET_ACCESS_KEY={{ backup_aws_secret_key }}
{% endif %}
{% if backup_aws_region %}
AWS_DEFAULT_REGION={{ backup_aws_region }}
{% endif %}
